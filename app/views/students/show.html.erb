<h2> Courses for Student: <%= @student.first_name %> <%= @student.last_name %>
 </h2>

<hr>

<%= link_to 'Edit Yourself', edit_student_path %>
<%= link_to("Delete Yourself", @student, :method => :delete, :class => 'action', data: {confirm: "Are you sure?"}) %>

 <table>
   <tr>
     <th>
       COURSE SUBJECT
     </th>
   </tr>

   <tr>
     <td>
       <% @student.courses.each do |course| %>
       <%= course.subject %> <br>
       <% end %>
     </td>
   </tr>
 </table>
